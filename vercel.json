{
  "version": 2,
  "routes": [
    {
      "src": "/api/lyrics",
      "methods": ["GET", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type, Authorization",
        "Cache-Control": "no-store"
      },
      "dest": "/api/lyrics.js"
    },
    {
      "src": "/api/lyrics-page",
      "methods": ["GET", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type, Authorization",
        "Cache-Control": "no-store"
      },
      "dest": "/api/lyrics-page.js"
    }
  ]
}
